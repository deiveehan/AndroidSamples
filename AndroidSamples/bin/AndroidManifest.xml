                <action android:name="com.cv.androidsamples.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cv.androidsamples.FormInputs"
            android:label="@string/title_activity_form_inputs" >
